KAEMI,cAFN,EAGMF,KAHN;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MAKGO,IAAI,CAACyB,iBAAL,CAAuBvB,SAAvB,EAAkC;AACjCc,IAAAA,KAAK,EAALA,KADiC;AAEjCb,IAAAA,YAAY,EAAZA,YAFiC;AAGjCM,IAAAA,aAAa,EAAbA;AAHiC,GAAlC,EAKEC,QAAQ,KAAK,QAAd,GACE,oBAAC,KAAD;AAAO,IAAA,IAAI,EAAC,QAAZ;AAAqB,IAAA,MAAM,EAAEC,IAA7B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IADF,GAEGnB,QAAQ,IAAI,oBAAC,KAAD,oBAAWgB,UAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAPhB,CALH,CADF;AAiBD;;;;;;;;;;;;;;;;;;;;;;0BA3EuBlB,Q","sourcesContent":["import * as React from 'react';\nimport Input from 'antd/es/input';\nimport Form from 'antd/es/form';\nimport 'antd/es/input/style';\nimport 'antd/es/form/style';\n\nexport default function FormItem({ className, children, ...props }) {\n  const { cols = [] } = props;\n  const {\n    formItemLayout = {\n      labelCol: { sm: { span: cols[0] || 10 } },\n      wrapperCol: { sm: { span: cols[1] || 14 } },\n    },\n    form,\n    label = '',\n    fieldName = '',\n    initialValue = undefined,\n    required = true,\n    validatorCallback = () => {},\n    extraRules = [],\n    inputProps = {},\n    valuePropName = 'value',\n    itemType = 'default',\n    unit,\n    isNegative = false,\n    isInteger = false,\n    message = '必填项',\n  } = props;\n\n  if (!form) {\n    return (\n      <Form.Item\n        style={{ marginBottom: 0 }}\n        className={className}\n        {...formItemLayout}\n        {...props}\n        label={label}\n      >\n        {children || initialValue || ' -- '}\n      </Form.Item>\n    );\n  }\n\n  const rules = [{ required, message }, {\n    validator: (rule, value, callback) => {\n      validatorCallback(value, callback, rule);\n\n      // 当类型为数字类型时，则内置校验规则\n      if (itemType === 'number') {\n        if (!isNegative && value < 0) {\n          callback(`${label} 不能小于 0`);\n          return;\n        }\n\n        if (isInteger && value && !Number.isInteger(parseFloat(value))) {\n          callback('必须为整数');\n          return;\n        }\n      }\n\n      callback();\n    },\n  }].concat(extraRules);\n\n  return (\n    <Form.Item\n      className={classNames(className)}\n      {...formItemLayout}\n      {...props}\n    >\n      {form.getFieldDecorator(fieldName, {\n        rules,\n        initialValue,\n        valuePropName,\n      })(\n        (itemType === 'number') ?\n          <Input type=\"number\" suffix={unit} /> :\n          (children || <Input {...inputProps} />)\n      )}\n    </Form.Item>\n  );\n}"]}]}